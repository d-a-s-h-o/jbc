}{TEXT -1 25 " triangle with vertices (" }{XPPEDIT 18 0 "x[1],y[1];" "
6$&%\"xG6#\"\"\"&%\"yG6#\"\"\"" }{TEXT -1 4 "), (" }{XPPEDIT 18 0 "x[2
],y[2];" "6$&%\"xG6#\"\"#&%\"yG6#\"\"#" }{TEXT -1 7 ") and (" }
