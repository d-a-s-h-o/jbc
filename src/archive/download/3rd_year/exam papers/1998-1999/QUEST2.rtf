local ss, mm, ll, pp, ii, ans; mm := nn;\par
ll := floor(1/2*trunc(evalf(log10(mm))))+1;\par
ans := ``; for ii to ll do mm := mm/100;\par
pp := 100*frac(mm);\par
ss := substring(`crypt/alphabet`, pp..pp);\par
ans := cat(ss, ans); mm := trunc(mm)\par
od; ans end:
}
