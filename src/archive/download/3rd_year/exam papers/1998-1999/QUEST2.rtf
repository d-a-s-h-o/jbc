{\cs12\f2\fs24\cf1\i0 \b \ul0 to_number := proc(st, string) \par
local ll, nn, ss, ii;  \par
ll := length(st);\par
if ll = 0 then RETURN(0) fi; \par
nn := 1;\par
for ii to ll do \par
ss := SearchText(substring(st, ii .. ii),`crypt/alphabet`);\par
nn := 100*nn + ss \par
od;\par
nn - 10^(2*ll) \par
end:
}
\par}
